<bean id="address1" class="com.javafullstacktraining.Address">  
	<property name="addressLine1" value="manikonda"></property>  
	<property name="city" value="hyd"></property>  
	<property name="state" value="ts"></property>  
	<property name="country" value="India"></property>  
</bean>  
  
<bean id="e1" class="com.javafullstacktraining.Employee">  
	<property name="id" value="11"></property>  
	<property name="name" value="Kumar"></property>  
	<property name="address" ref="address1"></property>  
</bean>

</beans>